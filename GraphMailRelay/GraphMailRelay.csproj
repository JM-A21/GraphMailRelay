		<PackageReference Include="McGuireV10.GenericHostBuilderLogger" Version="1.0.0" />
		<PackageReference Include="Microsoft.Extensions.Hosting" Version="7.0.1" />
		<PackageReference Include="Microsoft.Graph" Version="5.3.0" />
		<PackageReference Include="MimeKit" Version="3.6.1" />
		<PackageReference Include="SmtpServer" Version="9.0.2" />
	</ItemGroup>
</Project>
